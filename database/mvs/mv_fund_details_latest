SELECT DISTINCT ON (d.fcode) d.idtefasfunddetails,
    d.fcode,
    d.fdate,
    d.bankbills,
    d.exchangetradedfund,
    d.other,
    d.fxpayablebills,
    d.governmentbond,
    d.foreigncurrencybills,
    d.eurobonds,
    d.commercialpaper,
    d.fundparticipationcertificate,
    d.realestatecertificate,
    d.venturecapitalinvestmentfundparticipation,
    d.realestateinvestmentfundparticipation,
    d.treasurybill,
    d.stock,
    d.governmentbondsandbillsfx,
    d.participationaccount,
    d.participationaccountau,
    d.participationaccountd,
    d.participationaccounttl,
    d.governmentleasecertificates,
    d.governmentleasecertificatesd,
    d.governmentleasecertificatestl,
    d.governmentleasecertificatesforeign,
    d.preciousmetals,
    d.preciousmetalsbyf,
    d.preciousmetalskba,
    d.preciousmetalskks,
    d.publicdomesticdebtinstruments,
    d.privatesectorleasecertificates,
    d.privatesectorbond,
    d.repo,
    d.derivatives,
    d.tmm,
    d.reverserepo,
    d.assetbackedsecurities,
    d.termdeposit,
    d.termdepositau,
    d.termdepositd,
    d.termdeposittl,
    d.futurescashcollateral,
    d.foreigndebtinstruments,
    d.foreigndomesticdebtinstruments,
    d.foreignprivatesectordebtinstruments,
    d.foreignexchangetradedfunds,
    d.foreignequity,
    d.foreignsecurities,
    d.foreigninvestmentfundparticipationshares,
    d.privatesectorinternationalleasecertificate,
    d.privatesectorforeigndebtinstruments,
    lf.ftitle,
    lf.price AS current_price,
    lf.investorcount,
    lf.fcapacity
   FROM (tefasfunddetails d
     JOIN mv_latest_fund_data lf ON (((d.fcode)::text = (lf.fcode)::text)))
  ORDER BY d.fcode, d.fdate DESC